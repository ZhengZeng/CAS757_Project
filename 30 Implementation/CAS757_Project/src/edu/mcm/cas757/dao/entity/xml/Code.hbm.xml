<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="edu.mcm.cas757.dao.entity.Code" table="codes">
	
		<id name="id" type="java.lang.Integer" column="CodeId">
			<generator class="native"></generator>
		</id>

		<property name="lookupCode" type="java.lang.Integer"
			column="LookupCode">
		</property>
		<property name="value" type="string" column="CodeValue"></property>
		<property name="desc" type="string" column="CodeDescription"></property>
		<property name="defunct" type="java.lang.Boolean" column="Defunct">
		</property>
	</class>
</hibernate-mapping>