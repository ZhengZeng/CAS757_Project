<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="edu.mcm.cas757.dao.entity.Role" table="role">
		<id name="id" type="java.lang.Integer" column="RoleId">
			<generator class="native"></generator>
		</id>

		<property name="name" type="string" column="RoleName">
		</property>

		<property name="desc" type="string" column="Description"></property>

		<property name="createDate" type="string" column="CreateDate">
		</property>

		<property name="updateDate" type="string" column="UpdateDate">
		</property>

		<property name="creatorId" type="java.lang.Integer"
			column="CreatorId">
		</property>

		<property name="updateBy" type="java.lang.Integer"
			column="UpdateBy">
		</property>

	</class>

</hibernate-mapping>